package org.drools.simple.candrink

import org.submarine.quickstart.Person
import org.submarine.quickstart.Result

rule R1 when
        $r : Result()
        $p : Person( age >= 18 )
    then
        $r.setValue( $p.getName() + " can drink");
end

rule R2 when
        $r : Result()
        $p : Person( age < 18 )
    then
        $r.setValue( $p.getName() + " can NOT drink");
end

