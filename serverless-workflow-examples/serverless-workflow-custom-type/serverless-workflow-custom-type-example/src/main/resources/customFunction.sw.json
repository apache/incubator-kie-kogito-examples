{
  "id": "customFunction",
  "version": "1.0",
  "name": "Custom RPC example",
  "description": "This test a custom function can be added as addon in the classpath",
  "start": "start",
  "states": [
    {
      "name": "start",
      "type": "operation",
      "actions": [
       {
          "functionRef": {
             "refName": "rpc:division",
             "arguments": {
                 "dividend": ".dividend",
                 "divisor" : ".divisor"
             }
           }
          
       }
      ],
      "end": true
 	}
  ]
}